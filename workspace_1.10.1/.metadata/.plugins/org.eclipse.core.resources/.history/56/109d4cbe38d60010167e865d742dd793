/* USER CODE BEGIN 0 */
int pr1 = 0, pr2 = 0, pr3 = 0, pr4 = 0, pr5 = 0, pr6 = 0, pr7 = 0, pr8 = 0,
    pr9 = 0, pr10 = 0, pr11 = 0, pr12 = 0, pr13 = 0, pr14 = 0, pr15 = 0, pr16 = 0;

int klav() {
    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_6, 0);
    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_7, 1);
    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_8, 1);
    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_9, 1);

    if (HAL_GPIO_ReadPin(GPIOB, GPIO_PIN_5) != GPIO_PIN_SET) {
        if (pr1 == 0) return 1;
        pr1 = 1;
    } else pr1 = 0;
    if (HAL_GPIO_ReadPin(GPIOB, GPIO_PIN_4) != GPIO_PIN_SET) {
        if (pr2 == 0) return 2;
        pr2 = 1;
    } else pr2 = 0;
    if (HAL_GPIO_ReadPin(GPIOB, GPIO_PIN_3) != GPIO_PIN_SET) {
        if (pr3 == 0) return 3;
        pr3 = 1;
    } else pr3 = 0;
    if (HAL_GPIO_ReadPin(GPIOA, GPIO_PIN_15) != GPIO_PIN_SET) {
        if (pr4 == 0) return 4;
        pr4 = 1;
    } else pr4 = 0;

    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_6, 1);
    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_7, 0);
    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_8, 1);
    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_9, 1);

    if (HAL_GPIO_ReadPin(GPIOB, GPIO_PIN_5) != GPIO_PIN_SET) {
        if (pr5 == 0) return 5;
        pr5 = 1;
    } else pr5 = 0;
    if (HAL_GPIO_ReadPin(GPIOB, GPIO_PIN_4) != GPIO_PIN_SET) {
        if (pr6 == 0) return 6;
        pr6 = 1;
    } else pr6 = 0;
    if (HAL_GPIO_ReadPin(GPIOB, GPIO_PIN_3) != GPIO_PIN_SET) {
        if (pr7 == 0) return 7;
        pr7 = 1;
    } else pr7 = 0;
    if (HAL_GPIO_ReadPin(GPIOA, GPIO_PIN_15) != GPIO_PIN_SET) {
        if (pr8 == 0) return 8;
        pr8 = 1;
    } else pr8 = 0;

    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_6, 1);
    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_7, 1);
    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_8, 0);
    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_9, 1);

    if (HAL_GPIO_ReadPin(GPIOB, GPIO_PIN_5) != GPIO_PIN_SET) {
        if (pr9 == 0) return 9;
        pr9 = 1;
    } else pr9 = 0;
    if (HAL_GPIO_ReadPin(GPIOB, GPIO_PIN_4) != GPIO_PIN_SET) {
        if (pr10 == 0) return 10;
        pr10 = 1;
    } else pr10 = 0;
    if (HAL_GPIO_ReadPin(GPIOB, GPIO_PIN_3) != GPIO_PIN_SET) {
        if (pr11 == 0) return 11;
        pr11 = 1;
    } else pr11 = 0;
    if (HAL_GPIO_ReadPin(GPIOA, GPIO_PIN_15) != GPIO_PIN_SET) {
        if (pr12 == 0) return 12;
        pr12 = 1;
    } else pr12 = 0;

    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_6, 1);
    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_7, 1);
    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_8, 1);
    HAL_GPIO_WritePin(GPIOB, GPIO_PIN_9, 0);

    if (HAL_GPIO_ReadPin(GPIOB, GPIO_PIN_5) != GPIO_PIN_SET) {
        if (pr13 == 0) return 13;
        pr13 = 1;
    } else pr13 = 0;
    if (HAL_GPIO_ReadPin(GPIOB, GPIO_PIN_4) != GPIO_PIN_SET) {
        if (pr14 == 0) return 14;
        pr14 = 1;
    } else pr14 = 0;
    if (HAL_GPIO_ReadPin(GPIOB, GPIO_PIN_3) != GPIO_PIN_SET) {
        if (pr15 == 0) return 15;
        pr15 = 1;
    } else pr15 = 0;
    if (HAL_GPIO_ReadPin(GPIOA, GPIO_PIN_15) != GPIO_PIN_SET) {
        if (pr16 == 0) return 16;
        pr16 = 1;
    } else pr16 = 0;

    return 0;
}

#define BOARD_WIDTH 10
#define BOARD_HEIGHT 20

uint8_t board[BOARD_HEIGHT][BOARD_WIDTH] = {0};

typedef struct {
    int x, y;
    int blocks[4][2];
    uint16_t color;
} Tetromino;

const int SHAPES[7][4][2] = {
    {{0, -1}, {0, 0}, {0, 1}, {0, 2}},       // I
    {{0, 0}, {1, 0}, {0, 1}, {1, 1}},        // O
    {{-1, 0}, {0, 0}, {1, 0}, {0, 1}},       // T
    {{-1, 1}, {0, 1}, {0, 0}, {1, 0}},       // S
    {{-1, 0}, {0, 0}, {0, 1}, {1, 1}},       // Z
    {{-1, 1}, {-1, 0}, {0, 0}, {1, 0}},      // J
    {{-1, 0}, {0, 0}, {1, 0}, {1, 1}}        // L
};

// Используем прямые RGB565 значения — гарантируем совместимость
const uint16_t COLORS[7] = {
    0x07FF,  // CYAN  — I
    0xFFE0,  // YELLOW — O
    0xF81F,  // MAGENTA (фиолетовый) — T
    0x07E0,  // GREEN — S
    0xF800,  // RED — Z
    0x001F,  // BLUE — J
    0xFEA0   // ORANGE (31,28,0) — L
};

Tetromino current;
int next_shape_type;
int score = 0;
int gameover = 0;
int game_paused = 1;
volatile uint8_t update_flag = 0;

void draw_block(int x, int y, uint16_t color) {
    int px = x * 5 + 40;
    int py = y * 5 + 20;
    for (int dy = 0; dy < 5; dy++) {
        for (int dx = 0; dx < 5; dx++) {
            if (px + dx < 128 && py + dy < 160) {
                ST7735_DrawPixel(px + dx, py + dy, color);
            }
        }
    }
}

int can_move(int dx, int dy) {
    for (int i = 0; i < 4; i++) {
        int nx = current.x + current.blocks[i][0] + dx;
        int ny = current.y + current.blocks[i][1] + dy;
        if (nx < 0 || nx >= BOARD_WIDTH || ny >= BOARD_HEIGHT) return 0;
        if (ny >= 0 && board[ny][nx]) return 0;
    }
    return 1;
}

void rotate_piece() {
    if (game_paused || gameover) return;

    int rotated_blocks[4][2];
    for (int i = 0; i < 4; i++) {
        rotated_blocks[i][0] = -current.blocks[i][1];
        rotated_blocks[i][1] = current.blocks[i][0];
    }

    int valid = 1;
    for (int i = 0; i < 4; i++) {
        int nx = current.x + rotated_blocks[i][0];
        int ny = current.y + rotated_blocks[i][1];

        if (nx < 0 || nx >= BOARD_WIDTH || ny >= BOARD_HEIGHT) {
            valid = 0;
            break;
        }
        if (ny >= 0 && board[ny][nx]) {
            valid = 0;
            break;
        }
    }

    if (valid) {
        memcpy(current.blocks, rotated_blocks, sizeof(current.blocks));
    }
}

void rotate_piece_counter_clockwise() {
    if (game_paused || gameover) return;

    int rotated_blocks[4][2];
    for (int i = 0; i < 4; i++) {
        // Поворот против часовой стрелки: (x, y) -> (y, -x)
        rotated_blocks[i][0] = current.blocks[i][1];
        rotated_blocks[i][1] = -current.blocks[i][0];
    }

    int valid = 1;
    for (int i = 0; i < 4; i++) {
        int nx = current.x + rotated_blocks[i][0];
        int ny = current.y + rotated_blocks[i][1];

        if (nx < 0 || nx >= BOARD_WIDTH || ny >= BOARD_HEIGHT) {
            valid = 0;
            break;
        }
        if (ny >= 0 && board[ny][nx]) {
            valid = 0;
            break;
        }
    }

    if (valid) {
        memcpy(current.blocks, rotated_blocks, sizeof(current.blocks));
    }
}

void reset_game() {
    memset(board, 0, sizeof(board));
    score = 0;
    gameover = 0;
    game_paused = 1;  // Игра на паузе после сброса

    next_shape_type = rand() % 7;
    memcpy(current.blocks, SHAPES[next_shape_type], sizeof(current.blocks));
    current.color = COLORS[next_shape_type];
    current.x = BOARD_WIDTH / 2 - 1;
    current.y = 0;

    if (!can_move(0, 0)) {
        gameover = 1;
        game_paused = 1;
    }
}

void place_piece() {
    for (int i = 0; i < 4; i++) {
        int bx = current.x + current.blocks[i][0];
        int by = current.y + current.blocks[i][1];
        if (by >= 0 && by < BOARD_HEIGHT && bx >= 0 && bx < BOARD_WIDTH) {
            board[by][bx] = next_shape_type + 1;
        }
    }

    int lines_cleared = 0;
    for (int y = BOARD_HEIGHT - 1; y >= 0; y--) {
        int full = 1;
        for (int x = 0; x < BOARD_WIDTH; x++) {
            if (!board[y][x]) { full = 0; break; }
        }
        if (full) {
            lines_cleared++;
            for (int yy = y; yy > 0; yy--) {
                for (int x = 0; x < BOARD_WIDTH; x++) {
                    board[yy][x] = board[yy-1][x];
                }
            }
            for (int x = 0; x < BOARD_WIDTH; x++) board[0][x] = 0;
            y++;
        }
    }

    score += lines_cleared * 100;

    next_shape_type = rand() % 7;
    memcpy(current.blocks, SHAPES[next_shape_type], sizeof(current.blocks));
    current.color = COLORS[next_shape_type];
    current.x = BOARD_WIDTH / 2 - 1;
    current.y = 0;

    if (!can_move(0, 0)) {
        gameover = 1;
        game_paused = 1;
    }
}

void new_game() {
    memset(board, 0, sizeof(board));
    score = 0;
    gameover = 0;
    game_paused = 0;

    next_shape_type = rand() % 7;
    memcpy(current.blocks, SHAPES[next_shape_type], sizeof(current.blocks));
    current.color = COLORS[next_shape_type];
    current.x = BOARD_WIDTH / 2 - 1;
    current.y = 0;

    if (!can_move(0, 0)) {
        gameover = 1;
        game_paused = 1;
    }
}

void draw() {
    for (int y = 0; y < BOARD_HEIGHT; y++) {
        for (int x = 0; x < BOARD_WIDTH; x++) {
            draw_block(x, y, field_color);
        }
    }

    for (int y = 0; y < BOARD_HEIGHT; y++) {
        for (int x = 0; x < BOARD_WIDTH; x++) {
            if (board[y][x] > 0 && board[y][x] <= 7) {
                draw_block(x, y, COLORS[board[y][x] - 1]);
            }
        }
    }

    if (!gameover && !game_paused) {
        for (int i = 0; i < 4; i++) {
            int bx = current.x + current.blocks[i][0];
            int by = current.y + current.blocks[i][1];
            if (bx >= 0 && bx < BOARD_WIDTH && by >= 0 && by < BOARD_HEIGHT) {
                draw_block(bx, by, current.color);
            }
        }
    }

    for (int i = 0; i < 128; i++) {
        ST7735_DrawPixel(i, 19, ST7735_BLACK);
        ST7735_DrawPixel(i, 20, ST7735_BLACK);
        ST7735_DrawPixel(i, 120, ST7735_BLACK);
        ST7735_DrawPixel(i, 121, ST7735_BLACK);
    }
    for (int i = 20; i < 122; i++) {
        ST7735_DrawPixel(38, i, ST7735_BLACK);
        ST7735_DrawPixel(39, i, ST7735_BLACK);
        ST7735_DrawPixel(89, i, ST7735_BLACK);
        ST7735_DrawPixel(90, i, ST7735_BLACK);
    }

    char buf[32];
    if (gameover) {
        sprintf(buf, "Game Over!      ");
        ST7735_WriteString(4, 142, buf, Font_11x18, 0xF800, ST7735_BLACK); // RED text
    } else if (game_paused) {
        sprintf(buf, "Press START     ");
        ST7735_WriteString(4, 142, buf, Font_11x18, 0xFFE0, ST7735_BLACK); // YELLOW text
    } else {
        sprintf(buf, "Score: %d       ", score);
        ST7735_WriteString(4, 142, buf, Font_11x18, ST7735_WHITE, ST7735_BLACK);
    }
}

void input() {
    int k = klav();

    if (k == 0) return;

    if (k == 1) {
        reset_game();
        return;
    }

    if (game_paused || gameover) {
        if (k == 7) {
            new_game();
        }
        return;
    }

    if (k == 5) {
        rotate_piece();
        return;
    }

    if (k == 9) {
        rotate_piece_counter_clockwise();
        return;
    }

    switch (k) {
        case 6:
            if (can_move(-1, 0)) current.x--;
            break;
        case 8:
            if (can_move(1, 0)) current.x++;
            break;
        case 3:
        case 11:
            if (can_move(0, 1)) current.y++;
            else place_piece();
            break;
        default:
            break;
    }
}

void logic() {
    if (game_paused || gameover) return;
    if (can_move(0, 1)) {
        current.y++;
    } else {
        place_piece();
    }
}

void HAL_TIM_PeriodElapsedCallback(TIM_HandleTypeDef *htim) {
    if (htim == &htim10) {
        update_flag = 1;
    }
}

/* USER CODE END 0 */

int main(void)
{
  HAL_Init();
  SystemClock_Config();
  MX_GPIO_Init();
  MX_DMA_Init();
  MX_SPI1_Init();
  MX_ADC1_Init();
  MX_TIM10_Init();

  ST7735_Init();

  ST7735_FillScreenFast(field_color);
  for (int i = 0; i < 128; i++) {
      ST7735_DrawPixel(i, 19, ST7735_BLACK);
      ST7735_DrawPixel(i, 20, ST7735_BLACK);
      ST7735_DrawPixel(i, 120, ST7735_BLACK);
      ST7735_DrawPixel(i, 121, ST7735_BLACK);
  }
  for (int i = 20; i < 122; i++) {
      ST7735_DrawPixel(38, i, ST7735_BLACK);
      ST7735_DrawPixel(39, i, ST7735_BLACK);
      ST7735_DrawPixel(89, i, ST7735_BLACK);
      ST7735_DrawPixel(90, i, ST7735_BLACK);
  }

  srand(HAL_GetTick());
  draw();

  HAL_TIM_Base_Start_IT(&htim10);

  while (1)
  {
    input();

    if (update_flag) {
        update_flag = 0;
        logic();
        draw();
    }

    HAL_Delay(5);
  }
}



