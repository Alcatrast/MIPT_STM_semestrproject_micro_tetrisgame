int joystick(int x, int y){
  // none
	int d = 0;
	// right
	if (x > 2048+600 && 2048 - 600 < y && y < 2048 + 600)
	{
	d = 8;
	}
	// left
	if (x < 2048-600 && 2048 - 600 < y && y < 2048 + 600)
	{
	d = 6;
	}
	//up
	if (2048 - 600 < x && x < 2048 + 600 && y > 2048+600)
	{
	d = 11;
	}
	//down
	if (2048 - 600 < x && x < 2048 + 600 && y < 2048-600)
	{
	d = 3;
	}
	if (HAL_GPIO_ReadPin(GPIOA, GPIO_PIN_12) == 0)
	{
	d = 13;
	}
	return d;

}

#define HEIGHT 27
#define WIDTH 24

int snakeTailX[24*30], snakeTailY[24*30];
int snakeTailLen = 1;
int gameover, key, score;
int x, y, fruitx, fruity;

int old_tail_x[24*27];
int old_tail_y[24*27];
int old_x, old_y, old_app_x, old_app_y;

/*
 void input() {
    if (kbhit()) {
        switch (tolower(getch())) {
        case 'a':
            if(key!=2)
            key = 1;
            break;
        case 'd':
            if(key!=1)
            key = 2;
            break;
        case 'w':
            if(key!=4)
            key = 3;
            break;
        case 's':
            if(key!=3)
            key = 4;
            break;
        case 'x':
            gameover = 1;
            break;
        }
    }
}
 */
int flag_k = 1;
void input() {
	int k=0;
	if (flag_k == 1){
		k = klav();
	}
	if (flag_k == -1){

		HAL_ADC_Start_DMA(&hadc1, adcData, 2);
		int x = adcData[0], y = adcData[1];
		k = joystick(x, y);
	}
	if(k==13){
		flag_k*=-1;
		key = 7;
	}
	if (k!=0 && k!=13){
		key = k;
	}
}

void setup() {
	snakeTailLen = 1;
	key = 7;
	ST7735_FillScreenFast(field_color);
	for(int i = 0; i < 161; i++){
		ST7735_DrawPixel(0, i, ST7735_BLACK);
		ST7735_DrawPixel(1, i, ST7735_BLACK);
		ST7735_DrawPixel(2, i, ST7735_BLACK);
		ST7735_DrawPixel(3, i, ST7735_BLACK);

		ST7735_DrawPixel(128, i, ST7735_BLACK);
		ST7735_DrawPixel(127, i, ST7735_BLACK);
		ST7735_DrawPixel(126, i, ST7735_BLACK);
		ST7735_DrawPixel(125, i, ST7735_BLACK);
	}
	for(int i = 4; i < 125; i++){
		ST7735_DrawPixel(i, 0, ST7735_BLACK);
		ST7735_DrawPixel(i, 1, ST7735_BLACK);
		ST7735_DrawPixel(i, 2, ST7735_BLACK);
		ST7735_DrawPixel(i, 3, ST7735_BLACK);
		ST7735_DrawPixel(i, 4, ST7735_BLACK);

		ST7735_DrawPixel(i, 160-16, ST7735_BLACK);
		ST7735_DrawPixel(i, 159-16, ST7735_BLACK);
		ST7735_DrawPixel(i, 158-16, ST7735_BLACK);
		ST7735_DrawPixel(i, 157-16, ST7735_BLACK);
		ST7735_DrawPixel(i, 156-16, ST7735_BLACK);
	}


    // Initial coordinates of the snake
    x = WIDTH / 2;
    y = HEIGHT / 2;
    snakeTailX[0] = x-1;
    snakeTailY[0] = y;
    // Initial coordinates of the fruit
    fruitx = rand() % WIDTH;
    fruity = rand() % HEIGHT;
    while (fruitx == 0)
        fruitx = rand() % WIDTH;

    while (fruity == 0)
        fruity = rand() % HEIGHT;

    score = 0;
}

void draw_kv(int x, int y, int color){
	for(int i = 0; i < 25; i++){
	ST7735_DrawPixel(x*5+4+i/5, y*5+5+i%5, color);
	}
}

void draw(){
	if (old_x != x || old_y != y){
			draw_kv(old_x, old_y, field_color);
			draw_kv(x, y, head_color);
		}
	for (int i = 0; i < snakeTailLen; i++) {
		int flag = 0;
		for (int j = 0; j < snakeTailLen; j++) {
			if (old_tail_x[i] == snakeTailX[j] && old_tail_y[i] == snakeTailY[j]){flag = 1;}
		}
		if (flag == 0){draw_kv(old_tail_x[i], old_tail_y[i], field_color);}
	    }
	for (int i = 0; i < snakeTailLen; i++) {
		int flag = 0;
		for (int j = 0; j < snakeTailLen; j++) {
			if (old_tail_x[j] == snakeTailX[i] && old_tail_y[j] == snakeTailY[i]){flag = 1;}
		}
		if (flag == 0){draw_kv(snakeTailX[i], snakeTailY[i], snake_color);}
		}
	if (old_app_x != fruitx || old_app_y != fruity){
		draw_kv(old_app_x, old_app_y, field_color);
		draw_kv(fruitx, fruity, fruit_color);
	}

	for(int i = 0; i < snakeTailLen; i++){
		old_tail_x[i] = snakeTailX[i];
		old_tail_y[i] = snakeTailY[i];
	}
	old_x = x;
	old_y = y;
	old_app_x = fruitx;
	old_app_y = fruity;

	char d[10];
    sprintf(d, "Score: %d           ", score);
    ST7735_WriteString(4, 142, d, Font_11x18, ST7735_WHITE, ST7735_BLACK);


}

void logic() {
    // Updating the coordinates for continous
    // movement of snake
    int prevX = snakeTailX[0];
    int prevY = snakeTailY[0];
    int prev2X, prev2Y;
    snakeTailX[0] = x;
    snakeTailY[0] = y;
    for (int i = 1; i < snakeTailLen; i++) {
        prev2X = snakeTailX[i];
        prev2Y = snakeTailY[i];
        snakeTailX[i] = prevX;
        snakeTailY[i] = prevY;
        prevX = prev2X;
        prevY = prev2Y;
    }

    // Changing the direction of movement of snake
    switch (key) {
    case 6:
        x--;
        break;
    case 8:
        x++;
        break;
    case 3:
        y--;
        break;
    case 11:
        y++;
        break;
    default:
        break;
    }

    if(key != 7){
		// If the game is over
		if (x < 0 || x >= WIDTH || y < 0 || y >= HEIGHT)
			setup();


		// Checks for collision with the tail (o)
		for (int i = 0; i < snakeTailLen; i++) {
			if (snakeTailX[i] == x && snakeTailY[i] == y)
				setup();
		}
    }

    // If snake reaches the fruit
    // then update the score
    if (x == fruitx && y == fruity) {
        fruitx = rand() % WIDTH;
        fruity = rand() % HEIGHT;
        while (fruitx == 0)
            fruitx = rand() % WIDTH;

        // Generation of new fruit
        while (fruity == 0)
            fruity = rand() % HEIGHT;
        score += 1;
        snakeTailX[snakeTailLen] = 1;
        snakeTailY[snakeTailLen] = 30;
         snakeTailLen++;
    }

}




